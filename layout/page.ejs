<% if (page.type == 'categories') { %>
    <%- partial('_partial/category', {category: true, index: false}) %>
<% } else if (page.type == 'tags') { %>
    <%- partial('_partial/tag', {tag: true, index: false}) %>
<% } else { %>
    <%- partial('_partial/article', {post: page, index: false}) %>
<% } %>
